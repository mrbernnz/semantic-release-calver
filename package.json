    "@babel/preset-env": "7.26.0",
    "@babel/preset-typescript": "7.26.0",
    "@semantic-release/github": "11.0.1",
    "@semantic-release/npm": "12.0.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.2",
    "@types/semantic-release": "20.0.6",
    "@types/semantic-release__error": "3.0.3",
    "@types/signale": "1.4.7",
    "babel-jest": "29.7.0",
    "date-fns": "4.1.0",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "semantic-release": "24.2.0",
    "semver": "7.6.3",
    "signale": "1.4.0",
    "ts-jest": "29.2.5",
    "typescript": "5.7.2"
